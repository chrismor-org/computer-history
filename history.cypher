MATCH (a:System)
    WITH a
        MATCH (b:Company)
            WHERE a.system_name = 'ATLAS' AND a.manufacturer = b.company_name
                CREATE (a)-[mf:MANUFACTURER]->(b)
                RETURN mf;
MATCH (a:System)
    WITH a
        MATCH (b:Year)
            WHERE a.system_name = 'ATLAS' AND a.introduced = b.year
                CREATE (a)-[yi:INTRODUCED]->(b)
                RETURN yi;

